set number
set expandtab
set shiftwidth=4
set tabstop=4
set autoindent
set cindent
"set smartindent
set showmatch
set matchtime=5
set hlsearch
set incsearch

syntax on
colorscheme default
"Cyan Magenta green
hi LineNr cterm=none ctermfg=cyan ctermbg=black guifg=#888888 guibg=#000000

set fileencoding=utf-8
set fileencodings=utf-8,gbk


